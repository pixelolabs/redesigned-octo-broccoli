{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region <div>.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
  ]
%}

<script src="https://alertbar.oit.duke.edu/alert.html" type="text/javascript"></script> 

<header class="js__main-header">
	<div class="container-1805 flex">
		<div class="top-section flex">
			<div class="logo-section none-980">
				<a href="{{ url('<front>') }}" rel="home" title="Duke Occupational & Environmental Safety Office"><img src="/{{ directory }}/images/logo-duke-health-expanded.png" alt="Duke Occupational & Environmental Safety Office"></a>
			</div>
			<div class="logo-section-outer block-980">
				<a href="{{ url('<front>') }}" rel="home" title="Duke Occupational & Environmental Safety Office">
					<div class="logo-section"><img src="/{{ directory }}/images/duke-health-logo.png" alt="Duke Occupational & Environmental Safety Office"/></div>
					<span>Occupational & Environmental Safety Office</span>
				</a>
			</div>
			{# header #}
			{{content}}
		</div>
	</div>
	<a class="mobile-hamburger js__mobile-hamburger"></a>
	<div
		class="menu-section js__menu-section">
		{# navigation #}
		{{ drupal_region('navigation') }}
		<a class="report-issue" href="#" title="Report an Issue">Report an Issue</a>
		<a class="mobile-close js__mobilemenu-close"></a>
	</div>
</div></header><script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

